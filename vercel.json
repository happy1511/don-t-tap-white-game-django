{
    "builds": [{
        "src": "Game/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" , "runtime": "python3.9" },
        "handler":"application"
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Game/wsgi.py"
        }
    ]
}
